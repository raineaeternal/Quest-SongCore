{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Quest-SongCore",
    "id": "songcore",
    "version": "1.1.12",
    "url": "https://github.com/raineio/quest-songcore",
    "additionalData": {
      "overrideSoName": "libsongcore.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "qpm qmod manifest",
        "pwsh ./scripts/createqmod.ps1 SongCore"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ]
    }
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.1.7",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "^3.0.0",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3701.0.0",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.34",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "lapiz",
      "versionRange": "^0.2.11",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.8",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "kaleb",
      "versionRange": "^0.1.9",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "libcryptopp",
      "versionRange": "^8.5.0",
      "additionalData": {
        "private": true
      }
    }
  ]
}
