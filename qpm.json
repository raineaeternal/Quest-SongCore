{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Quest-SongCore",
    "id": "songcore",
    "version": "1.1.12",
    "url": "https://github.com/raineio/quest-songcore",
    "additionalData": {
      "overrideSoName": "libsongcore.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "qpm qmod manifest",
        "pwsh ./scripts/createqmod.ps1 SongCore"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ]
    },
    "qmodIncludeDirs": [],
    "qmodIncludeFiles": [],
    "qmodOutput": null
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.0",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.1",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^4030.0.0",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.50",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "lapiz",
      "versionRange": "^0.2.17",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.2",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "kaleb",
      "versionRange": "^0.1.9",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "libcryptopp",
      "versionRange": "^8.5.0",
      "additionalData": {
        "private": true
      }
    }
  ]
}